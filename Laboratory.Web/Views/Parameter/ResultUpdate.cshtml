@model ParameterDto

<style>

    .form-container {
        display: flex;
        gap: 30px; /* spacing between columns */
    }

    .form-column {
        flex: 1; /* makes both columns equal width */
    }

    .form-group {
        margin-bottom: 15px;
    }

    label {
        display: block;
        margin-bottom: 5px;
        white-space: nowrap;
    }

    input {
        width: 100%;
        padding: 8px;
    }

    .topic-select-container {
        display: flex;
        align-items: center; /* Vertically center */
        gap: 105px; /* Space between topic and select */
        width: 1000px;
        /*         height: 100px;
            font-size: 18px;
            padding: 10px;
            border-radius: 20px; */ */
    }

    .topic-title {
        font-weight: bold;
        font-size: 20px;
        /*  height: 100px; */
        /*   display: inline-block;
            vertical-align: middle;
            margin-right: 10px; */
        white-space: nowrap;
        align-items: center;
        margin-block-start: 10px;
    }
</style>

@{
    ViewData["Title"] = "Pathology Test Entry";
    var testTypes = new List<SelectListItem>
    {
        new SelectListItem { Text = "Lipid Profile", Value = "Lipid" },
        new SelectListItem { Text = "UFR", Value = "UFR" },
        new SelectListItem { Text = "LFT", Value = "LFT" },
        new SelectListItem { Text = "FBC", Value = "FBC" },
        new SelectListItem { Text = "Rheumatoid Factor", Value = "RheumatoidFactor" },
        new SelectListItem { Text = "C Reactive Protein", Value = "CReactiveProtein" },
        new SelectListItem { Text = "ESR(westergren)1st hour ", Value = "ESR" },
        new SelectListItem { Text = "S CREATININE ", Value = "SCREATININE " },
        new SelectListItem { Text = "S.G.O.T.(A.S.T)", Value = "S.G.O.T.(A.S.T)" },
        new SelectListItem { Text = "S.G.P.T.(A.L.T)", Value = "S.G.P.T.(A.L.T)" },
        new SelectListItem { Text = "BLOOD GLUCOSE - Random ", Value = "BloodGlucoseR" },
        new SelectListItem { Text = "WIDAL(SAT)  Test", Value = "WIDAL(SAT)" },
        new SelectListItem { Text = "Haemoglobin  ", Value = "Haemoglobin" },
        new SelectListItem { Text = "Rhematoid Factor  ", Value = "RhematoidFactor" },
        new SelectListItem { Text = "BLOOD GLUCOSE - Fasting ", Value = "BloodGlucoseF" },
        new SelectListItem { Text = "BLOOD GLUCOSE - Breakfast ", Value = "BloodGlucoseB" },
        new SelectListItem { Text = "BLOOD GLUCOSE - Lunch ", Value = "BloodGlucoseL" },
        new SelectListItem { Text = "BLOOD GLUCOSE - Dinner ", Value = "BloodGlucoseD" },
    };
}


<form asp-action="ResultUpdate" enctype="multipart/form-data">
    <br />
    <div class="container border p-3">
        <h1 class="text-white text-center">Update Product</h1>
        <input hidden asp-for="Id" />
        <hr />
        <div class="form-container">
            <div class="form-column">
                <div class="form-group">
                    <div>
                        <label class="control-label pt-2" style="font-size:20px;"> Name</label>
                    </div>
                    <div>
                        <input asp-for="PatientName" placeholder="full name..." class="form-control" />
                        <span asp-validation-for="PatientName" class="text-danger"></span>
                    </div>
                </div>
                <div class="form-group">
                    <div>
                        <label class="control-label pt-2" style="font-size:20px;"> Age</label>
                    </div>
                    <div>
                        <input asp-for="Age" placeholder="age..." class="form-control" />
                        <span asp-validation-for="Age" class="text-danger"></span>
                    </div>
                </div>
                <div class="form-group">
                    <div>
                        <label class="control-label pt-2" style="font-size:20px;"> Sex</label>
                    </div>
                    <div>
                        <input asp-for="Sex" placeholder="sex..." class="form-control" />
                        <span asp-validation-for="Sex" class="text-danger"></span>
                    </div>
                </div>
                <div class="form-group">
                    <div>
                        <label class="control-label pt-2" style="font-size:20px;"> Ref Number</label>
                    </div>
                    <div>
                        <input asp-for="RefNumber" placeholder="reference number..." class="form-control" />
                        <span asp-validation-for="RefNumber" class="text-danger"></span>
                    </div>
                </div>
                <div>
                    <div>
                        <label class="control-label pt-2" style="font-size:20px;"> Lab Number</label>
                    </div>
                    <div>
                        <input asp-for="LabNumber" placeholder="lab number..." class="form-control" />
                        <span asp-validation-for="LabNumber" class="text-danger"></span>
                    </div>
                </div>
            </div>

            <div class="form-column">
                <div>
                    <label class="control-label pt-2" style="font-size:20px"> Collected Date</label>
                </div>
                <div>
                    <input asp-for="CollectedDate" placeholder="collected..." class="form-control" />
                    <span asp-validation-for="CollectedDate" class="text-danger"></span>
                </div>
                <div class="form-group">

                    <div class="form-group">
                        <div>
                            <label class="control-label pt-2" style="font-size:20px;margin-bottom: 20px"> Reported Date</label>
                        </div>
                        <div>
                            <input asp-for="ReportedDate" placeholder="reported date..." class="form-control" />
                            <span asp-validation-for="ReportedDate" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div>
                        <label class="control-label pt-2" style="font-size:20px;"> Date Of Birth</label>
                    </div>
                    <div>
                        <input asp-for="DOB" placeholder="Date of birth..." class="form-control" />
                        <span asp-validation-for="DOB" class="text-danger"></span>
                    </div>
                </div>
                <div class="form-group">
                    <div>
                        <label class="control-label pt-2" style="font-size:20px;"> Phone</label>
                    </div>
                    <div>
                        <input asp-for="Phone" placeholder="phone..." class="form-control" />
                        <span asp-validation-for="Phone" class="text-danger"></span>
                    </div>
                </div>
                <div class="form-group">
                    <div>
                        <label class="control-label pt-2" style="font-size:20px;">Email</label>
                    </div>
                    <div>
                        <input asp-for="Email" class="form-control" />
                        <span asp-validation-for="Email" class="text-danger"></span>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="form-group">

        <br />
        <div class="topic-select-container">
            <div class="topic-title">Test Type</div>
            <select id="testType" name="TestType" class="form-control" onchange="showTestFields()">
                <option value="">-- Select Test Type --</option>
                @foreach (var testType in testTypes)
                {
                    <option value="@testType.Value">@testType.Text</option>
                }
            </select>
        </div>
        </div>
    <br />

    <div id="dynamicFields">
        <!-- Dynamic fields will appear here -->
    </div>
    <br />
    @* <div style="display: flex; justify-content: center; align-items: center;">
        <span offset-10>
            <div class=" col-4" style="width: 150px; height: 50px;">
                <input type="submit" value="Update" class="btn btn-success form-control" />
            </div>

        </span>
    </div> *@
    <div class="row">
        <div class="col-6 offset-6; row-cols-1">

            <a asp-action="ParameterIndex" class="btn-primary btn form-control ">Back to List</a>
        </div>

        <div class="col-6">
        <input type="submit" value="Update" class="btn btn-success form-control" />
        </div>

        
    </div>
    @*    </div>
    </div> *@
    </div>

</form>





